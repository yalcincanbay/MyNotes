<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/mylogo"
        android:label="MyNotes"
        android:roundIcon="@mipmap/mylogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject"
        tools:targetApi="31">
        <activity
            android:name="com.example.yalcin.Projects"
            android:exported="false" />
        <activity
            android:name="com.example.yalcin.Languages"
            android:exported="false" />
        <activity
            android:name="com.example.yalcin.Films"
            android:exported="false" />
        <activity
            android:name="com.example.yalcin.Books"
            android:exported="false"/>

        <activity
            android:name="com.example.yalcin.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
